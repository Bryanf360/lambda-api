generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id    Int      @id @default(autoincrement())
  names      String
  lastnames  String
  email      String   @unique
  password   String
  role       String   @default("technician")
  status     String   @default("active")
  created_at DateTime @default(now())
}
